F00:0080       *************************************************************************************
F00:0081       *
F00:0082       * the following code is rosco_m68k-specific
F00:0083       
F00:0084       * output character to the console from register d0.b
F00:0085       
F00:0086       VEC_OUT:
F00:0087           move.l  D1,-(A7)              ; Save D1
               S01:00028002:  2F 01
F00:0088       .BEGIN
F00:0089           bset.b  #7,MFP_GPDR           ; Raise (inhibit) RTS
               S01:00028004:  08 F9 00 07 00 F8 00 01
F00:0090           move.b  MFP_TSR,D1            ; Get TSR
               S01:0002800C:  12 39 00 F8 00 2D
F00:0091           btst    #7,D1                 ; Is buffer empty bit set?
               S01:00028012:  08 01 00 07
F00:0092           beq.s   .BEGIN                ; No - loop until it is
               S01:00028016:  67 EC
F00:0093       
F00:0094           move.b  D0,MFP_UDR            ; Put func arg in UDR
               S01:00028018:  13 C0 00 F8 00 2F
F00:0095           move.l  (A7)+,D1              ; Restore D1
               S01:0002801E:  22 1F
F00:0096           rts
               S01:00028020:  4E 75
F00:0097       
F00:0098       *************************************************************************************
F00:0099       *
F00:0100       * input a character from the console into register d0
F00:0101       * else return Cb=0 if there's no character available
F00:0102       
F00:0103       VEC_IN:
F00:0104           move.b  MFP_RSR,D0            ; Get RSR
               S01:00028022:  10 39 00 F8 00 2B
F00:0105           btst    #7,D0                 ; Is buffer_full bit set?
               S01:00028028:  08 00 00 07
F00:0106           bne.s   .GOTCHR               ; Yes - Go to receive character
               S01:0002802C:  66 14
F00:0107       
F00:0108           btst    #6,D0                 ; Else, do we have an overrun error?
               S01:0002802E:  08 00 00 06
F00:0109           bne.s   .GOTERR               ; .. Yes - handle that
               S01:00028032:  66 06
F00:0110       
F00:0111           andi.b  #$FE,CCR              ; Else clear the carry flag, no character available
               S01:00028034:  02 3C 00 FE
F00:0112           rts                           ; And return
               S01:00028038:  4E 75
F00:0113       
F00:0114       .GOTERR
F00:0115           move.b  MFP_UDR,D0            ; Empty buffer
               S01:0002803A:  10 39 00 F8 00 2F
F00:0116           bra.s   VEC_IN                ; And continue testing...
               S01:00028040:  60 E0
F00:0117       
F00:0118       .GOTCHR
F00:0119           move.b  MFP_UDR,D0            ; Get the data
               S01:00028042:  10 39 00 F8 00 2F
F00:0120           ori.b   #1,CCR                ; Set the carry to flag we got a char
               S01:00028048:  00 3C 00 01
F00:0121           rts
               S01:0002804C:  4E 75
